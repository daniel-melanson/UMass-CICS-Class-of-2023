{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "lib": ["ESNext","DOM","DOM.Iterable"],
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "#discord/*": ["discord/*"],
      "#umass/*": ["umass/*"],
      "#shared/*": ["shared/*"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true        
  },
  "include": ["./src/**/*.ts"],
  "exclude": ["./node_modules"]
}
