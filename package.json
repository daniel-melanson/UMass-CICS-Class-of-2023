{
  "name": "umass-cics-community-bot",
  "author": "Daniel Melanson",
  "homepage": "https://github.com/daniel-melanson/umass-cics-community-bot#readme",
  "version": "3.0.0",
  "license": "AGPL-3.0",
  "description": "Discord chatbot for a college community server.",
  "bugs": {
    "url": "https://github.com/daniel-melanson/umass-cics-community-bot/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniel-melanson/umass-cics-community-bot.git"
  },
  "main": "./dist/index.js",
  "engines": {
    "node": ">=16.6.0"
  },
  "devDependencies": {
    "@types/node": "^16.7.1",
    "@types/node-cron": "^2.0.4",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "scripts": {
    "lint": "npx eslint src",
    "lint:fix": "npx eslint --fix src",
    "build": "npx tsc",
    "build:debug": "npm run clean && npx tsc --sourceMap",
    "build:watch": "npx tsc -w",
    "clean": "rm -rf ./dist",
    "start": "node ./dist/index.js",
    "start:debug": "npm run clean && npm run build:debug && npm run start",
    "start:prod": "npm run clean && npm run build && npm run start"
  },
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "cheerio": "^1.0.0-rc.10",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "mongodb": "^4.1.1",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.4",
    "ow": "^0.27.0",
    "ts-mixer": "^6.0.0",
    "tslib": "^2.3.1"
  },
  "imports": {
    "#umass/*": "./dist/umass/*.js",
    "#discord/*": "./dist/discord/*.js",
    "#shared/*": "./dist/shared/*.js"
  }
}
